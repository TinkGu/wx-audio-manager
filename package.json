{
  "name": "wx-audio-manager",
  "version": "1.1.0",
  "description": "微信小程序背景音频 backgroundAudio 封装",
  "license": "MIT",
  "repository": "TinkGu/wx-audio-manager",
  "main": "dist/wx-audio-manager.cjs.js",
  "module": "dist/wx-audio-manager.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "author": {
    "name": "TinkGu",
    "email": "tlwrite@126.com",
    "url": "https://github.com/TinkGu"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext js,ts,tsx",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc --emitDeclarationOnly && rollup -c",
    "prerelease": "npm run lint && npm run build",
    "release": "standard-version",
    "postpublish": "git push origin HEAD --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --ext js,ts,tsx --fix"
    ]
  },
  "keywords": [
    "wx",
    "weixin",
    "miniprogram",
    "backgroundAudio",
    "audioManager"
  ],
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@babel/preset-env": "7.13.8",
    "@babel/preset-typescript": "7.13.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.4.1",
    "@types/prop-types": "15.7.3",
    "@types/wechat-miniprogram": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "babel-eslint": "10.1.0",
    "eslint": "7.21.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.4",
    "open-cli": "6.0.1",
    "prettier": "2.2.1",
    "raf": "3.4.1",
    "rimraf": "3.0.2",
    "rollup": "2.40.0",
    "rollup-plugin-ignore": "1.0.9",
    "rollup-plugin-terser": "7.0.2",
    "standard-version": "9.1.1",
    "typescript": "4.2.2"
  }
}
